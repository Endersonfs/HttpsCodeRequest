<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>HttpsCodeRequest</PackageId>
    <Version>1.0.0</Version>
    <Authors>Enderson Florian Solano</Authors>
    <Company>Endprog</Company>
    <Description>Package to have all the HttpCode with their description</Description>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/Endersonfs/HttpsCodeRequest</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>httpcode</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
